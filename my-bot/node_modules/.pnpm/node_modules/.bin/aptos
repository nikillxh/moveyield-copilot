#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nikhilesh/Infinity/Blockchain/Movement/my-bot/node_modules/.pnpm/@aptos-labs+aptos-cli@1.1.1/node_modules/@aptos-labs/aptos-cli/dist/node_modules:/home/nikhilesh/Infinity/Blockchain/Movement/my-bot/node_modules/.pnpm/@aptos-labs+aptos-cli@1.1.1/node_modules/@aptos-labs/aptos-cli/node_modules:/home/nikhilesh/Infinity/Blockchain/Movement/my-bot/node_modules/.pnpm/@aptos-labs+aptos-cli@1.1.1/node_modules/@aptos-labs/node_modules:/home/nikhilesh/Infinity/Blockchain/Movement/my-bot/node_modules/.pnpm/@aptos-labs+aptos-cli@1.1.1/node_modules:/home/nikhilesh/Infinity/Blockchain/Movement/my-bot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nikhilesh/Infinity/Blockchain/Movement/my-bot/node_modules/.pnpm/@aptos-labs+aptos-cli@1.1.1/node_modules/@aptos-labs/aptos-cli/dist/node_modules:/home/nikhilesh/Infinity/Blockchain/Movement/my-bot/node_modules/.pnpm/@aptos-labs+aptos-cli@1.1.1/node_modules/@aptos-labs/aptos-cli/node_modules:/home/nikhilesh/Infinity/Blockchain/Movement/my-bot/node_modules/.pnpm/@aptos-labs+aptos-cli@1.1.1/node_modules/@aptos-labs/node_modules:/home/nikhilesh/Infinity/Blockchain/Movement/my-bot/node_modules/.pnpm/@aptos-labs+aptos-cli@1.1.1/node_modules:/home/nikhilesh/Infinity/Blockchain/Movement/my-bot/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@aptos-labs/aptos-cli/dist/aptos.js" "$@"
else
  exec node  "$basedir/../@aptos-labs/aptos-cli/dist/aptos.js" "$@"
fi
